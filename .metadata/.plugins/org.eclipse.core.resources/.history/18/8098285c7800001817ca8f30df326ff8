          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SCL5gAB3EeiUg4x9yHMHjA" name="removeComponent" specification="_SCLScAB3EeiUg4x9yHMHjA">
            <language>JAVA</language>
            <body>
    Component removeThis = null;
    for (// look up the right component, by comparing names
    Component c : // look up the right component, by comparing names
    components) {
        if (c.getName().equals(name)) {
            removeThis = c;
        }
    }
    if (removeThis != null) {
        components.remove(removeThis);
    }
</body>
