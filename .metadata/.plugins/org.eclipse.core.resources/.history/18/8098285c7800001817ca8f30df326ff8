    boolean exists = false;
    for (//see if component does not already exist
    Component c : //see if component does not already exist
    components) {
        if (c.getName().equals(name)) {
            exists = true;
            break;
        }
    }
    if (// only add component if it does not already exist
    !exists) {
        components.add(new Component(name));
    }
</body>
