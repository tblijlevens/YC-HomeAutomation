          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SCQK8QB3EeiUg4x9yHMHjA" name="createNewAutomation" specification="_SCNusAB3EeiUg4x9yHMHjA">
            <language>JAVA</language>
            <body>
    boolean exists = false;
    for (//see if automation does not already exist
    Automation a : //see if automation does not already exist
    automations) {
        if (a.getName().equals(autoName)) {
            exists = true;
        }
    }
    if (//only makes an automation if it does not already exist
    !exists) {
        Component triggerComp = null;
        Component responseComp = null;
        for (// find the right components by comparing names
        Component c : // find the right components by comparing names
        components) {
            if (c.getName().equals(triggerCompName)) {
                triggerComp = c;
            } else if (c.getName().equals(responseCompName)) {
                responseComp = c;
            }
        }
        if (// if both components are found
        triggerComp != null &amp;&amp; responseComp != null) {
            // create new automation
            Automation newAutomation = new Automation(autoName, triggerComp, triggerStatus, responseComp, responseStatus);
            // add automation to ArrayList &quot;automations&quot;
            automations.add(newAutomation);
            //returns succes feedback
            return true;
        }
    }
    // returns fail feedback
    return false;
</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SCSnMQB3EeiUg4x9yHMHjA" name="removeAutomation" specification="_SCRZEAB3EeiUg4x9yHMHjA">
            <language>JAVA</language>
            <body>
    Automation removeThis = null;
    for (// find the right automation by comparing names
    Automation a : // find the right automation by comparing names
    automations) {
        if (a.getName().equals(name)) {
            removeThis = a;
        }
    }
    if (removeThis != null) {
        automations.remove(removeThis);
    }
</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SCVqgAB3EeiUg4x9yHMHjA" name="changeStatus" specification="_SCT1UAB3EeiUg4x9yHMHjA">
            <language>JAVA</language>
            <body>
    String triggeredAutomations = &quot;&quot;;
    ArrayList&lt;String> allChainedAutos = new ArrayList&lt;>();
    ArrayList&lt;String> chainedAutos = new ArrayList&lt;>();
    // Loop through all components to see if status needs to be changed and consequently if there are automations that need to be triggered because of this status change:
    for (// loop through all components
    Component c : // loop through all components
    components) {
        if (//check if the wanted status is not the current status (only execute status change if wanted status is different than current status)
        c.getName() == name &amp;&amp; c.getStatus() != stat) {
            //change status for the right component
            c.changeStatus(stat);
            // find all automations that this status change should trigger: when component and status are triggers in an automation.
            for (//loop through automations
            Automation a : //loop through automations
            automations) {
                if (//find automations that have both component and status as its triggerComp and triggerStatus.
                a.getTriggerComp() == c &amp;&amp; a.getTriggerStatus() == stat) {
                    // trigger that automation. chainedAutos now contains an ArrayList of Strings of all names of all chained automations
                    chainedAutos = a.triggerAutomation(automations);
                    for (//loop through chainedAutos
                    String s : //loop through chainedAutos
                    chainedAutos) {
                        //add all names from chainedAutos to allChainedAutos (chainedAutos will be re-initialised in every loop, losing the names, allChainedAutos will contain all names added each loop and keep it until the end of the method)
                        allChainedAutos.add(s);
                    }
                }
            }
        }
    }
    // after all automations have been triggered add all names of all triggered automations to a String.
    for (String s : allChainedAutos) {
        triggeredAutomations += s + &quot;\n&quot;;
    }
    // can be used by gui to give feedback to user
    return triggeredAutomations;
</body>
          </ownedBehavior>
